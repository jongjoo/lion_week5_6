



<div class="container">
  <h1 >자유 게시판 </h1>
    <form action="/create" method="POST">
      <div class="form-group">
        <label>제목</label>
        <input type="text"  name="title" class="form-control" id="title" placeholder="제목입력">
      </div>
        
        <div class="form-group">
          <label for="comment">내용</label>
          <textarea class="form-control" name="content" rows="5" id="content"></textarea>
        </div>

      <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>



<div class="container">
    <%@view.each do |p|%>
    <div class="container">
        
    <hr>
  
    제목 : <%=p.title%><Br>
    내용 : <%=p.content%><br>
    <hr>
    
    <hr>
    </div>
    <%end%>
    
    
    
</div>